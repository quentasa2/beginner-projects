nums=[4,1,2,-3]
sort=False
n=1
for i in range(len(nums)):
    nums[i]=nums[i]*nums[i]
while not sort:
    try:
        if nums[n]<nums[n-1] and n>0:
            nums[n],nums[n-1]=nums[n-1],nums[n]
            n-=1
        else:
            n+=1
    except:
        n+=1
        if n==len(nums)+1:
            sort=True
print(nums)
